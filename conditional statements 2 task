#include<stdio.h>
int main()
{
int number; 
printf("enter a number(1-5):");
scanf("%d",&number );
switch(number)
{ case 1:printf("Food Item -Sandwich\n");
	    printf("Price-Rs 149\n");
	    break;	

  case 2:printf("Food Item - Pasta\n");
	    printf("Price-Rs 179\n");
	    break;


  case 3:printf("Food Item - Burger\n");
	    printf("Price-Rs 129\n");
	    break;

  case 4:printf("Food Item - Pizza\n");
	    printf("Price-Rs 239\n");
	    break;

  case 5:printf("Food Item - French Fries\n");
     	printf("Price-Rs 99\n");
	    break;
}
return 0;	
}
